{
  "name": "rdf2json",
  "version": "0.0.0",
  "description": "Transforms RDF files from rdf-files.tar.bz2 into JS objects",
  "license": "ISC",
  "author": "Chris Newton",
  "type": "module",
  "bin": "dist/cli.js",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "postbuild": "chmod +x dist/cli.js",
    "pretest": "npm run build",
    "test": "node --test --enable-source-maps",
    "predry-run": "npm run build",
    "dry-run": "tar -lxOf input-files/rdf-files.tar.bz2 | node --enable-source-maps dist/cli.js - output-data"
  },
  "dependencies": {
    "fast-xml-parser": "^5.0.7",
    "html-entities": "^2.5.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "typescript": "^5.7.3"
  }
}
